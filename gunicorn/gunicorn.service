[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target
[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/EliteNest1
ExecStart=/home/ubuntu/env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/EliteNest1/EliteNest/EliteNest.sock EliteNest.wsgi:application

       
[Install]
WantedBy=multi-user.target